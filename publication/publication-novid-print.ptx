        <baseurl href="https://opentext.uleth.ca/apex-calculus"/>
    </html>
    <!-- PDF-Specific Options -->

    <latex print="yes" sides="two">
      <asymptote links="no"/>
    </latex>
    <epub cover="../cover/cover.png"/>
</publication>
