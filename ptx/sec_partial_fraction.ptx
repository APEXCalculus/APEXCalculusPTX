        <exercise label="ex-partial-fractions-defint-4">
          <webwork xml:id="webwork-ex-partial-fractions-defint-4">
            <pg-code>
              $a = 0;
              $b = 1;
              $F = Formula("-(2x+1)/(2*(x+1)^2)");
              Context("Fraction");
              $r = Fraction($F-&gt;eval(x=&gt;$b) - $F-&gt;eval(x=&gt;$a));
            </pg-code>
            <statement>
              <p>
                <m>\ds \int_{0}^1 \frac{x}{(x+1)(x^2+2x+1)}\, dx</m>
              </p>
              <p>
                <var name="$r" width="20"/>
              </p>
            </statement>
          </webwork>
        </exercise>
