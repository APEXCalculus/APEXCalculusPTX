      <exercisegroup cols="2" xml:id="exset-partial-fractions-defint">
        <introduction>
          <p>
            Evaluate the definite integral.
          </p>
        </introduction>
        <!-- Exercise 27 -->
        <exercise label="ex-partial-fractions-defint-1">
          <webwork xml:id="webwork-ex-partial-fractions-defint-1">
            <pg-code>
              $a = 1;
              $b = 2;
              ($r,$s) = num_sort((-9..-1,3..9)[NchooseK(16,2)]);
              ($A,$B) = (1..9)[NchooseK(9,2)];
              if($envir{problemSeed}==1){$r=-3;$s=-2;$A=3;$B=5;};
              Context("Fraction");
              Context()-&gt;flags-&gt;set(reduceConstantFunctions=&gt;0);
              $f = Formula("(($A+$B)x-($A*$s+$B*$r))/((x-$r)(x-$s))")-&gt;reduce;
              $arg = Fraction(abs($b-$r)**$A * abs($b-$s)**$B * abs($a-$r)**(-$A) * abs($a-$s)**(-$B));
              $answer = Formula("ln($arg)");
            </pg-code>
            <statement>
              <p>
                <m>\ds \int_{<var name="$a"/>}^{<var name="$b"/>} <var name="$f"/>\, dx</m>
              </p>
              <p>
                <var name="$answer" width="20"/>
              </p>
            </statement>
          </webwork>
        </exercise>
        <!-- Exercise 28 -->
        <exercise label="ex-partial-fractions-defint-2">
          <webwork xml:id="webwork-ex-partial-fractions-defint-2">
            <pg-code>
              $a = 0;
              $b = random(2,9,1);
              $r = random(-9,-1,1);
              ($m,$n) = (2..5)[NchooseK(4,2)];
              ($A,$B) = (-9..-1,1..9)[NchooseK(18,2)];
              if($envir{problemSeed}==1){$b=5;$r=-4;$m=3;$n=2;$A=5;$B=-1;};
              Context("Fraction");
              Context()-&gt;flags-&gt;set(reduceConstantFunctions=&gt;0);
              $f = Formula("(($A*$m+$B)x+($A*$n-$B*$r))/(($m x + $n)(x-$r))")-&gt;reduce;
              $F = Formula("$A ln(abs(x-$r)) + $B/$m ln(abs($m x+$n))")-&gt;reduce;
              $answer = $F-&gt;eval(x=&gt;$b) - $F-&gt;eval(x=&gt;$a);
            </pg-code>
            <statement>
              <p>
                <m>\ds \int_{<var name="$a"/>}^{<var name="$b"/>} <var name="$f"/>\, dx</m>
              </p>
              <p>
                <var name="$answer" width="20"/>
              </p>
            </statement>
          </webwork>
        </exercise>
        <!-- Exercise 29 -->
        <exercise label="ex-partial-fractions-defint-3">
          <webwork xml:id="webwork-ex-partial-fractions-defint-3">
            <pg-code>
              $a = -1;
              $b = 1;
              $h = non_zero_random(-4,4,1);
              $r = list_random(-10..-2,2..10);
              $A = random(2,5,1);
              $B = random(2,5,1);
              $C = non_zero_random(-5,5,1);
              if($envir{problemSeed}==1){$h=-2;$r=10;};
              $f = Formula("(x^2 + (1-2*$h)x + (($h)^2+1-$r))/((x-$r)(x^2+(-2*$h) x+(($h)^2+1)))")-&gt;reduce;
              Context("Fraction");
              Context()-&gt;flags-&gt;set(reduceConstantFunctions=&gt;0);
              $F = Formula("ln(abs(x-$r)) + atan(x-$h)")-&gt;reduce;
              $frac=Fraction($b-$r,$a-$r);
              $Fb = Formula("atan($b-$h)");
              if ($b-$h == 0) {$Fb = 0;};
              if ($b-$h == 1) {$Fb = Formula("pi/4");};
              if ($b-$h == -1) {$Fb = Formula("-pi/4");};
              $Fa = Formula("atan($a-$h)");
              if ($a-$h == 0) {$Fa = 0;};
              if ($a-$h == 1) {$Fa = Formula("pi/4");};
              if ($a-$h == -1) {$Fa = Formula("-pi/4");};
              $Fba = $Fb - $Fa;
              $Fba = $Fb if ($Fa == 0);
              $Fba = -$Fa if ($Fb == 0);
              $answer = Formula("ln($frac) + $Fba");
            </pg-code>
            <statement>
              <p>
                <m>\ds \int_{<var name="$a"/>}^{<var name="$b"/>} <var name="$f"/>\, dx</m>
              </p>
              <p>
                <var name="$answer" width="20"/>
              </p>
            </statement>
          </webwork>
        </exercise>
