        <!-- Exercise 23 -->
        <exercise label="ex-partial-fractions-evaluate-17">
          <webwork xml:id="webwork-ex-partial-fractions-evaluate-17">
            <pg-code>
              $k = list_random(4,9,16,25);
              $r = non_zero_random(-9,9,1);
              $a = random(2,9,1);
              ($b,$c) = (-9..-1,1..9)[NchooseK(18,2)];
              if($envir{problemSeed}==1){$k=9;$r=-1;$a=2;$b=1;$c=1;};
              $sqrtk = sqrt($k);
              Context("Fraction");
              $C = Fraction(($c*$r - $k*$a*$r - $b*$k)/-$sqrtk/(($r)**2+$k));
              $B = ($b*$k - $C*$k*$sqrtk)/(-2*$r*$k);
              $A = $a - 2*$B;
              $f = Formula("(($A+2*$B)x^2 + (-2*$B*$r + $C*$sqrtk)x + ($A*$k - $r*$C*$sqrtk))/((x-$r)(x^2+$k))")-&gt;reduce;
              Context()-&gt;flags-&gt;set(reduceConstantFunctions=&gt;0);
              $F = FormulaUpToConstant("$A ln(abs(x-$r)) + $B ln(x^2+$k) + $C atan(x/$sqrtk)")-&gt;reduce;
              $F-&gt;{test_at} = [[$r-0.25],[$r+0.25]];
            </pg-code>
            <statement>
              <p>
                <m>\ds \int <var name="$f"/> \, dx</m>
              </p>
              <p>
                <var name="$F" width="40"/>
              </p>
            </statement>
          </webwork>
        </exercise>
