            <pg-code>
              ($r,$s) = (-9..-1,1..9)[NchooseK(18,2)];
              ($A,$B) = (-3..-1,1..3)[NchooseK(6,2)];
              if($envir{problemSeed}==1){$r=1;$s=-2;$A=1;$B=-1;};
              $f = Formula("(x^2+($A+$B-$r-$s)x+($r*$s-$A*$s-$B*$r))/(x^2-($r+$s)x+$r*$s)")-&gt;reduce;
              Context("Fraction");
              $F = FormulaUpToConstant("x + $A ln(abs(x-$r)) + $B ln(abs(x-$s))")-&gt;reduce;
              $F-&gt;{test_at} = [[$r-0.25],[$r+0.25],[$s-0.25],[$s+0.25]];
            </pg-code>
            <statement>
