              @y=(random(0.01,0.99,0.01),random(0.01,0.99,0.01),random(0.01,0.99,0.01),random(0.01,0.99,0.01),random(0.01,0.99,0.01));
              @x=map{(1-($_)**(2/5))**(5/2)}(@y);
              @xy=map{[$x[$_],$y[$_]]}(0..4);
              $ddyddx-&gt;{test_points}=~~@xy;
            </pg-code>
            <statement>
