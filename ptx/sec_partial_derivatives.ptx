                  dot((-0.5,0.25,0.065));
                  draw((-1.5,0.25,0.815)--(0.5,0.25,-0.685),dashed+bluepen);

                  //Draw the tangent lines in x direction, slope -0.25 and dot
                  //  L=(-0.5,0.75,-0.6875)+t(1,0,-0.25) for t=-1,1
                  dot((-0.5,0.75,-0.6875));
                  draw((-1.5,0.75,-0.4375)--(0.5,0.75,-0.9375),dashed+bluepen);

