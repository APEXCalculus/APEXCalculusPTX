              ($lowd,$highd) = (3,4,6)[NchooseK(3,2)];
              if ($trig eq 'sin') {
                if (list_random(0,1)) {$low = Formula("pi/$lowd"); $high = Formula("(($highd-1)pi)/$highd")}
