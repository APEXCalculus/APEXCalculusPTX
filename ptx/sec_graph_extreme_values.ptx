              $min = min($f-&gt;eval(x=&gt;$low-&gt;eval(x=&gt;0)),$f-&gt;eval(x=&gt;$high-&gt;eval(x=&gt;0)),$c * $f-&gt;eval(x=&gt;$low-&gt;eval(x=&gt;0)) / abs($f-&gt;eval(x=&gt;$low-&gt;eval(x=&gt;0))));
