              $max = Fraction(max($f-&gt;eval(x=&gt;$low),$f-&gt;eval(x=&gt;$high),$f-&gt;eval(x=&gt;-$b/2)));
              $min = Fraction(min($f-&gt;eval(x=&gt;$low),$f-&gt;eval(x=&gt;$high),$f-&gt;eval(x=&gt;-$b/2)));
