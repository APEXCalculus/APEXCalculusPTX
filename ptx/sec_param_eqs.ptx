              </pg-code> -->
              <statement>
                <p>
                  <m>\ds x=r\cos(t)</m>, <m>\ds y=r\sin(t)</m>
                </p>
              </statement>
              <solution>
                <p>
                  <m>x^2+y^2=r^2</m>;
                  circle centered at <m>(0,0)</m> with radius <m>r</m>.
                </p>
              </solution>
          <!-- </webwork> -->
        </exercise>

        <exercise label="ex-parametric-describe-rectangular-3">
          <!-- <webwork xml:id="webwork-ex-parametric-describe-rectangular-3">
