              <statement>
                <p>
                  <m>\ds x=at+x_0</m>, <m>\ds y=bt+y_0</m>
                </p>
              </statement>
              <solution>
                <p>
                  <m>y=\frac{b}{a}(x-x_0)+y_0</m>;
                  line through <m>(x_0,y_0)</m> with slope <m>b/a</m>.
                </p>
              </solution>
          <!-- </webwork> -->
        </exercise>

        <exercise label="ex-parametric-describe-rectangular-2">
          <!-- <webwork xml:id="webwork-ex-parametric-describe-rectangular-2">
