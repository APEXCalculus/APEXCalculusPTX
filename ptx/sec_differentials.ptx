        <exercise label="ex-differentials-survey-5">
           <webwork xml:id="webwork-ex-differentials-survey-5">
            <pg-code>
              $theta = 143;
              $d = 50;
              $f = Compute("2*x*tan( ($theta*pi/180) /2)");
              $l = $f-&gt;eval(x=&gt;$d);
              $lU = NumberWithUnits("$l ft");
              $df = $f-&gt;D('x');
              $err = $df-&gt;eval(x=&gt;$d) * 0.5;
