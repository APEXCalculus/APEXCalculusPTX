              $m=Fraction($df-&gt;eval(x=&gt;$x0));
              Context()-&gt;variables-&gt;add(y=&gt;'Real');
              parser::Assignment-&gt;Allow;
              $t=Formula("y=$m (x-$x0)+$y0")-&gt;reduce;
              if ($m == 0) {
                $n=Formula("x=$x0");
              }
              else {
                $mn = Fraction(-1/$m);
                $n=Formula("y=$mn (x-$x0)+$y0")-&gt;reduce;
              }
