              $B = Fraction(1,2*$b) * (abs($b)/$b);
              ($Anum,$Aden) = $A-&gt;value;
              ($Bnum,$Bden) = $B-&gt;value;
              $A = Formula("$Anum/($Aden pi)");
              $B = Formula("$Bnum/($Bden pi)");
              $F = FormulaUpToConstant("$A sin($a pi x) - $B sin($b pi x)")-&gt;reduce;
            </pg-code>
            <statement>
              <p>
                <m>\ds \int <var name="$f"/> \, dx</m>
              </p>
              <p>
                <var name="$F" width="40"/>
              </p>
            </statement>
