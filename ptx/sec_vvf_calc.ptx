            \draw [-&gt;,thick,secondcolor] (A.center) -- ($(A)!1.2!(B)$) node [above,black,pos=.99] { $\displaystyle\frac{\vec r(t_1)-\vec r(t_0)}{t_1-t_0}$};

            \draw [-&gt;,thick,secondcolor] (A.center)--($(A)!.4!29:(B)$) node [above,black] { $\vrp(t_0)$};

