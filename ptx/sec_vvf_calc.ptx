            \draw [thick,-&gt;] (0,0) -- (B.center) node [below,pos=.5] { $\vec r(t_1)$};
            \draw [-&gt;,thick,secondcolor] (A.center) -- (B.center) node [above,black,pos=.5,sloped] { $\vec r(t_1)-\vec r(t_0)$};

            \end{tikzpicture}

            </latex-image>
