      <exercisegroup cols="2" xml:id="exset-multi-chain-with-data">

        <introduction>
          <p>
            Find <m>\lz{z}{t}</m>,
            or <m>\plz{z}{s}</m> and <m>\plz{z}{t}</m>,
            using the supplied information.
          </p>
        </introduction>

        <exercise label="ex-multi-chain-with-data-1">
          <!-- <webwork xml:id="webwork-ex-multi-chain-with-data-1">
              <pg-code>
              </pg-code> -->
              <statement>
                <p>
                  <m>\ds\frac{\partial z}{\partial x} = 2</m>,<m>\ds\frac{\partial z}{\partial y} = 1</m>,<m>\ds\frac{dx}{dt} = 4</m>,<m>\ds\frac{dy}{dt} = -5</m>
                </p>
              </statement>
              <solution>
                <p>
                  <m>\frac{dz}{dt} = 2(4)+1(-5) = 3</m>.
                </p>
              </solution>
          <!-- </webwork> -->
        </exercise>

        <exercise label="ex-multi-chain-with-data-2">
          <webwork xml:id="webwork-ex-multi-chain-with-data-2">
              <pg-code>
                $dzdt=Compute("0");
              </pg-code>

              <statement>
                <p>
                  <m>\plz{z}{x} = 1</m>,
                  <m>\plz{z}{y} = -3</m>, <m>\lz{x}{t} = 6</m>, and <m>\lz{y}{t} = 2</m>.
                </p>
                <instruction>
                  Find <m>\lz{z}{t}</m>:
                </instruction>
                <p>
