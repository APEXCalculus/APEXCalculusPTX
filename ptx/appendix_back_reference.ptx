<appendix xml:id="appendix-reference">
  <title>Quick Reference</title>

  <section xml:id="reference-derivatives">
    <title>Differentiation Formulas</title>
    <list xml:id="reference-deriv-rules">
      <title>Derivative Rules</title>
      <ol cols="3">
        <li>
          <m>\lz{cx}{x}=c</m>
        </li>

        <li>
          <m>\lz{u\pm v}{x}=u'\pm v'</m>
        </li>

        <li>
          <m>\lz{u\cdot v}{x}=uv'+ u'v</m>
        </li>

        <li>
          <m>\lz{\frac uv}{x}=\frac{vu'-uv'}{v^2}</m>
        </li>

        <li>
          <m>\lz{u(v)}{x}=u'(v)v'</m>
        </li>

        <li>
          <m>\lz{c}{x}=0</m>
        </li>

        <li>
          <m>\lz{x}{x}=1</m>
        </li>
      </ol>
    </list>

    <list xml:id="reference-deriv-elementary">
      <title>Derivatives of Elementary Functions</title>
      <ol cols="3">
        <li>
          <m>\lz{x^n}{x}=nx^{n-1}</m>
        </li>

        <li>
          <m>\lz{e^x}{x}=e^x</m>
        </li>

        <li>
          <m>\lz{a^x}{x}=\ln a\cdot a^x</m>
        </li>

        <li>
          <m>\lz{\ln x}{x}=\frac{1}{x}</m>
        </li>

        <li>
          <m>\lz{\log_a x}{x}=\frac{1}{\ln a}\cdot \frac1x</m>
        </li>

        <li>
          <m>\lz{\sin x}{x}=\cos x</m>
        </li>

        <li>
          <m>\lz{\cos x}{x}=-\sin x</m>
        </li>

        <li>
          <m>\lz{\csc x}{x}=-\csc x\cot x</m>
        </li>

        <li>
          <m>\lz{\sec x}{x}=\sec x\tan x</m>
        </li>

        <li>
          <m>\lz{\tan x}{x}=\sec^2 x</m>
        </li>

        <li>
          <m>\lz{\cot x}{x}=-\csc^2 x</m>
        </li>

        <li>
          <m>\lz{\cosh x}{x}=\sinh x</m>
        </li>

        <li>
          <m>\lz{\sinh x}{x}=\cosh x</m>
        </li>

        <li>
          <m>\lz{\tanh x}{x}=\sech^2 x</m>
        </li>

        <li>
          <m>\lz{\sech x}{x}=-\sech x\tanh x</m>
        </li>

        <li>
          <m>\lz{\csch x}{x}=-\csch x\coth x</m>
        </li>

        <li>
          <m>\lz{\coth x}{x}=-\csch^2 x</m>
        </li>
      </ol>
    </list>

    <list xml:id="reference-deriv-inverse">
      <title>Derivatives of Inverse Functions</title>
      <ol cols="3">
        <li>
          <m>\lz{\sin^{-1}x}{x}=\frac{1}{\sqrt{1-x^2}}</m>
        </li>

        <li>
          <m>\lz{\cos^{-1}x}{x}=\frac{-1}{\sqrt{1-x^2}}</m>
        </li>

        <li>
          <m>\lz{\csc^{-1}x}{x}=\frac{-1}{\abs{x}\sqrt{x^2-1}}</m>
        </li>

        <li>
          <m>\lz{\sec^{-1}x}{x}=\frac{1}{\abs{x}\sqrt{x^2-1}}</m>
        </li>

        <li>
          <m>\lz{\tan^{-1}x}{x}=\frac{1}{1+x^2}</m>
        </li>

        <li>
          <m>\lz{\cot^{-1}x}{x}=\frac{-1}{1+x^2}</m>
        </li>

        <li>
          <m>\lz{\cosh^{-1}x}{x}=\frac1{\sqrt{x^2-1}}</m>
        </li>

        <li>
          <m>\lz{\sinh^{-1}x}{x}=\frac1{\sqrt{x^2+1}}</m>
        </li>

        <li>
          <m>\lz{\sech^{-1}x}{x}=\frac{-1}{x\sqrt{1-x^2}}</m>
        </li>

        <li>
          <m>\lz{\csch^{-1}x}{x}=\frac{-1}{\abs{x}\sqrt{1+x^2}}</m>
        </li>

        <li>
          <m>\lz{\tanh^{-1}x}{x}=\frac1{1-x^2}</m>
        </li>

        <li>
          <m>\lz{\coth^{-1}x}{x}=\frac1{1-x^2}</m>
        </li>
      </ol>
    </list>
  </section>

  <section xml:id="reference-integration">
    <title>Integration Formulas</title>

    <list xml:id="reference-int-rules">
    <title>Basic Rules</title>
      <ol cols="2">
        <li>
          <m>\int c\cdot f(x)\,dx=c\int f(x)\,dx</m>
        </li>

        <li>
          <m>\int \bigl(f(x)\pm g(x)\bigr)\,dx = \int f(x)\, dx \pm \int g(x)\, dx</m>
        </li>

        <li>
          <m>\int 0\,dx = C</m>
        </li>

        <li>
          <m>\int 1\,dx=x+C</m>
        </li>

        <li>
          <m>\int x^n\,dx=\frac{1}{n+1}x^{n+1}+C, n\neq -1</m>
        </li>

        <li>
          <m>\int e^x\,dx=e^x+C</m>
        </li>

        <li>
          <m>\int \ln x\,dx=x\ln x -x +C</m>
        </li>

        <li>
          <m>\int a^x\,dx=\frac{1}{\ln a}\cdot a^x+C</m>
        </li>

        <li>
          <m>\int \frac{1}{x}\,dx =\ln \abs{x} + C</m>
        </li>
      </ol>
    </list>

    <list xml:id="reference-int-trig">
      <title>Integrals Involving Trigonometric Functions</title>
      <ol cols="2">
        <li>
          <m>\int \cos x\,dx=\sin x+C</m>
        </li>

        <li>
          <m>\int \sin x\,dx=-\cos x+C</m>
        </li>

        <li>
          <m>\int \tan x\,dx=-\ln \abs{\cos x}+C</m>
        </li>

        <li>
          <m>\int \sec x\,dx=\ln \abs{\sec x+\tan x}+C</m>
        </li>

        <li>
          <m>\int \csc x\,dx=-\ln \abs{\csc x+\cot x}+C</m>
        </li>

        <li>
          <m>\int \cot x\,dx=\ln \abs{\sin x}+C</m>
        </li>

        <li>
          <m>\int \sec^2 x\,dx=\tan x+C</m>
        </li>

        <li>
          <m>\int \csc^2x\,dx=-\cot x+C</m>
        </li>

        <li>
          <m>\int \sec x\tan x\,dx=\sec x+C</m>
        </li>

        <li>
          <m>\int \csc x\cot x\,dx=-\csc x+C</m>
        </li>

        <li>
          <m>\int \cos^2x\,dx=\frac12x+\frac14\sin\big(2x\big)+C</m>
        </li>

        <li>
          <m>\int \sin^2x\,dx=\frac12x-\frac14\sin\big(2x\big)+C</m>
        </li>

        <li>
          <m>\int \frac{1}{x^2+a^2}\,dx = \frac1a\tan^{-1}\left(\frac xa\right)+C</m>
        </li>

        <li>
          <m>\int \frac{1}{\sqrt{a^2-x^2}} = \sin^{-1}\left(\frac xa\right)+C</m>
        </li>

        <li>
          <m>\int \frac{1}{x\sqrt{x^2-a^2}} = \frac1a\sec^{-1}\left(\frac{\abs{x}}{a}\right)+C</m>
        </li>
      </ol>
    </list>

    <list xml:id="reference-int-hyper">
      <title>Integrals Involving Hyperbolic Functions</title>
      <ol cols="2">
        <li>
          <m>\int \cosh x\,dx=\sinh x+C</m>
        </li>

        <li>
          <m>\int \sinh x\,dx=\cosh x+C</m>
        </li>

        <li>
          <m>\int \tanh x\,dx=\ln(\cosh x)+C</m>
        </li>

        <li>
          <m>\int \coth x\,dx=\ln \abs{\sinh x}+C</m>
        </li>

        <li>
          <m>\int \frac{1}{\sqrt{x^2-a^2}} =\ln\big\abs{x+\sqrt{x^2-a^2}\big}+C</m>
        </li>

        <li>
          <m>\int \frac{1}{\sqrt{x^2+a^2}}=\ln\big\abs{x+\sqrt{x^2+a^2}\big}+C</m>
        </li>

        <li>
          <m>\int \frac{1}{a^2-x^2}} =\frac1{2a}\ln\left\abs{\frac{a+x}{a-x}\right}+C</m>
        </li>

        <li>
          <m>\int \frac{1}{x\sqrt{a^2-x^2}} = \frac1a\ln\left(\frac{x}{a+\sqrt{a^2-x^2}}\right)+C</m>
        </li>

        <li>
          <m>\int \frac{1}{x\sqrt{x^2+a^2}} = \frac1a\ln\left\abs{\frac{x}{a+\sqrt{x^2+a^2}}\right}+C</m>
        </li>
      </ol>
    </list>
  </section>

