                                      (axis cs: 3,0) -- (axis cs:3,.4) -- (axis cs: 4,.4) -- (axis cs: 4,0)
                                      (axis cs: 4,0) -- (axis cs:4,.333) -- (axis cs: 5,.333) -- (axis cs: 5,0);

