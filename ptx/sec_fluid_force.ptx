                  \draw [right color=firstcolor!15,left color=firstcolor!40,thick] (AAA.center) -- (BBB.center) -- (CCC.center)--(DDD.center) -- cycle;

                  \draw [thick] (AA.center) -- (BB.center) -- (CC.center) -- (DD.center) -- cycle
                                (AA.center) -- (AAA.center)
                                (CC.center) -- (CCC.center)
                                (B.center) -- (BB.center)
                                (DD.center) --(DDD.center);

                  \draw [thick,dashed]	(DDD.center)--(D.center) -- (C.center)
                                        (D.center) -- (A.center)
                                        (O.center) circle (2)
                                        (O.center) -- node [pos=.5,above,rotate=15] { 2 ft} (OC.center);

                  \begin{scope}[shift={(5.5,3)}]

                    \draw (-.5,0) -- (.5,0)
                          (-.5,10) -- (.5,10)
                          (0,0) -- node [pos=.5,fill=white,rotate=90] { 10 ft} (0,10);

