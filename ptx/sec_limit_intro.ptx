        <webwork xml:id="webwork-TaC-limit-intro-2">
          <pg-code>
            $answer = "an indeterminate form";
            $undefined = "an undefined expression";
            Context()-&gt;strings-&gt;add(
              $answer =&gt; {},
              indeterminate =&gt; {alias =&gt; $answer},
