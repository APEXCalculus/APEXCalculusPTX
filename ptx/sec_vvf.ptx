        <exercise label="ex-vector-function-magnitude-1">
          <webwork xml:id="webwork-ex-vector-function-magnitude-1">
              <pg-code>
                Context()-&gt;variables-&gt;are(t=&gt;'Real');
                $norm=Formula("|t| sqrt(1+t^2)");
              </pg-code>

              <statement>
                <p>
                  If <m>\vec r(t) = \la t,t^2\ra</m>,
                  then <m>\norm{\vec r(t)}=</m><var name="$norm" width="30"/>.
                </p>
              </statement>
          </webwork>
        </exercise>

        <exercise label="ex-vector-function-magnitude-2">
          <!-- <webwork xml:id="webwork-ex-vector-function-magnitude-2">
              <pg-code>
              </pg-code> -->
              <statement>
                <p>
                  <m>\vec r(t) = \la 5\cos(t) ,3\sin(t) \ra</m>.
                </p>
              </statement>
              <solution>
                <p>
                  <m>\norm {\vec r(t)} = \sqrt{25\cos^2(t) +9\sin^2(t) }</m>.
                </p>
