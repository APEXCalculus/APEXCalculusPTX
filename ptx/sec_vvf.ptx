                </row>
                <row bottom="minor">
                  <cell/>
                  <cell/>
                  <cell/>
                </row>
                <row>
                  <cell><m>-2</m></cell>
                  <cell><m>-6</m></cell>
                  <cell>1/5</cell>
                </row>
                <row>
                  <cell><m>-1</m></cell>
                  <cell><m>0</m></cell>
                  <cell>1/2</cell>
                </row>
                <row>
                  <cell><m>0</m></cell>
                  <cell><m>0</m></cell>
                  <cell><m>1</m></cell>
                </row>
                <row>
                  <cell><m>1</m></cell>
                  <cell><m>0</m></cell>
                  <cell>1/2</cell>
                </row>
