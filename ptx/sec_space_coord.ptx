          label("\noindent \centering In plane\\ $x=0$",(0,2,6),N);//label("$x=0$",(0,2,5.5),N);
          draw((0,2.25,5.5)--(0,2,4.25),linewidth(.75),Arrow3);
          label("\noindent \centering In plane\\ $z=d$",(-1,3,1),N);//label("$z=d$",(-1,2,0.5),E);
          draw((-1,3,1.75)--(-2.1,1.1,2),linewidth(.75),Arrow3);

        </asymptote>
      </image>
