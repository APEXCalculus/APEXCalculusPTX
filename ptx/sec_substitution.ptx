              if($envir{problemSeed}==1){$b=-5;$lb=1;$ub=3};
              Context("Fraction");
              Context()-&gt;flags-&gt;set(reduceConstantFunctions=&gt;0);
              $f = Formula("1/(x+$b)")-&gt;reduce;
              $frac = Fraction($ub+$b,$lb+$b);
              $A = Formula("ln($frac)");
            </pg-code>
            <statement>
              <p>
                <m>\ds \int_{<var name="$lb"/>}^{<var name="$ub"/>} <var name="$f"/> \, dx</m>
              </p>
              <p>
                <var name="$A" width="40"/>
              </p>
            </statement>
          </webwork>
        </exercise>
