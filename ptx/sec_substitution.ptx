              $f = Formula("sin^$n(x) cos(x)");
              $ftex = '\sin^' . $n . '(x)\cos(x)';
              $A = Fraction(1/($n+1)*sin(Real($ub))**($n+1) - 1/($n+1)*sin(Real($lb))**($n+1));
              if ($trig eq 'cos') {
                $f = Formula("cos^$n(x) sin(x)");
                $ftex = '\cos^' . $n . '(x)\sin(x)';
                $A = Fraction(-1/($n+1)*cos(Real($ub))**($n+1) + 1/($n+1)*cos(Real($lb))**($n+1));
              }
              $lb = Formula("$lb");
              $ub = Formula("$ub");
            </pg-code>
            <statement>
              <p>
