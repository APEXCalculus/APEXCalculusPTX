              $f = Formula("($A x^($m-1) + $B x^($n-1))($u)^$p")-&gt;reduce;
              $F = FormulaUpToConstant("1/(($p+1)*$g) ($u)^($p+1)")-&gt;reduce;
            </pg-code>
