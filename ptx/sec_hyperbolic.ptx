                $df = $f-&gt;D('x');
                $m = $df-&gt;eval(x=&gt;$x0);
                $y0 = $f-&gt;eval(x=&gt;$x0);
                $line = Formula("$m*(x - $x0) + $y0");
              </pg-code>

              <statement>
                <p>
                  Find the equation of the tangent line to <m>y=f(x)</m> at <m>x=0</m>, where <m>f(x) = \sinh(x)</m>.
                </p>

                <p>
                  <m>y = </m><var name="$line" width="20"/>
                </p>
              </statement>
              <solution>
              <p>
                <m>y=x</m>
              </p>

              </solution>
          </webwork>
        </exercise>
  <!-- Exercise 23 -->
