    <subexercises>
      <title>Problems</title>
      <exercisegroup cols="2" xml:id="exset-riemann-write-sum">
        <introduction>
          <p>
            Write out each term of the summation and compute the sum.
          </p>
        </introduction>
        <!-- Exercise 5 -->
        <exercise label="ex-riemann-write-sum-1">
          <webwork xml:id="webwork-ex-riemann-write-sum-1">
            <pg-code>
              $lb = random(2,5,1);
