            <solution>
              <p>
                <md>
                  <mrow>\lim\limits_{x\to <var name="$a"/>}<var name="$f"/>\amp=<var name="$b"/>^{\lim\limits_{x\to <var name="$a"/>}\left(x^3- <var name="$c"/>x\right)}</mrow>
                  <mrow>\amp=<var name="$b"/>^{\lim\limits_{x\to <var name="$a"/>}x^3-\lim\limits_{x\to <var name="$a"/>}<var name="$c"/>x}</mrow>
                  <mrow>\amp=<var name="$b"/>^{\left(\lim\limits_{x\to <var name="$a"/>}x\right)^3- <var name="$c"/>\lim\limits_{x\to <var name="$a"/>}x}</mrow>
                  <mrow>\amp=<var name="$b"/>^{<var name="$a"/>^3- <var name="$c"/>\cdot <var name="$a"/>}</mrow>
                  <mrow>\amp=<var name="$L"/></mrow>
