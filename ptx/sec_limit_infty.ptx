        <exercise label="ex-limit-infinity-numerical-1">
          <webwork xml:id="webwork-ex-limit-infinity-numerical-1">
            <pg-code>
              @b=(-9..9)[NchooseK(19,4)];
              if($envir{problemSeed}==1){@b=(3,1,-1,-2);};
              $a=$b[0];
              $f=Formula("(x^2-($b[1]+$b[2])*x+$b[1]*$b[2])/(x^2-($a+$b[3])*x+$a*$b[3])")->reduce;
