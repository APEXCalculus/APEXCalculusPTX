                \addplot [firstcurvestyle,domain=.5:3,samples=50] ({sqrt(x)},{sin(deg(x))});
                \addplot [firstcurvestyle,domain=3:18,samples=120] ({sqrt(x)},{sin(deg(x))});

                \filldraw (axis cs: 0.808252, 0.607787) circle (2.4pt)
                          (axis cs: 1.81447, -0.150147) circle (2.4pt)
